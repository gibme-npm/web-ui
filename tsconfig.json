{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "es2017",
      "dom",
      "scripthost"
    ],
    "module": "commonjs",
    "declaration": true,
    "outDir": "./dist",
    "strict": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "types": [
      "jquery",
      "bootstrap",
      "datatables.net-bs5",
      "google.visualization",
      "google.maps"
    ]
  },
  "include": [
    "src/**/*"
  ]
}
